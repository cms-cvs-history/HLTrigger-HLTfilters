# Lange -- 060804 

import popen2
import subprocess
import os

evs= [100, 20000, 50000]
nf= [2, 10, 30]
np= [1, 5, 10, 50, 100, 200]

for i in evs:
  for j in nf:
    for k in np:
      cstr="./manyHlt -nhlt %s -nev %s -nfilt %s" % (str(k),str(i),str(j))	
      print cstr
      p = subprocess.Popen(cstr, shell=True)
      sts = os.waitpid(p.pid, 0)

